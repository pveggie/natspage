<%= link_to "Add photo", new_photo_path %>
<div class="photo-holder">
  <% @photos.each do |photo| %>
    <div class="photo">

      <%= image_tag(photo.image_location, class: photo)%>
        <div class="caption">
          <h5><%= photo.caption_title %></h5>
          <p><%= photo.caption_description %></p>
        </div>
    </div>
  <% end %>
</div>
